s.delete(`${BASE_URL}delete-expense/${id}`)\n        getExpenses()\n    }\n\n    const totalExpenses = () => {\n        let totalIncome = 0;\n        expenses.forEach((income) =>{\n            totalIncome = totalIncome + income.amount\n        })\n\n        return totalIncome;\n    }\n\n\n    const totalBalance = () => {\n        return totalIncome() - totalExpenses()\n    }\n\n    const transactionHistory = () => {\n        const history = [...incomes, ...expenses]\n        history.sort((a, b) => {\n            return new Date(b.createdAt) - new Date(a.createdAt)\n        })\n\n        return history.slice(0, 3)\n    }\n\n\n    return (\n        <GlobalContext.Provider value={{\n            addIncome,\n            getIncomes,\n            incomes,\n            deleteIncome,\n            expenses,\n            totalIncome,\n            addExpense,\n            getExpenses,\n            deleteExpense,\n            totalExpenses,\n            totalBalance,\n            transactionHistory,\n            error,\n            setError\n        }}>\n            {children}\n        </GlobalContext.Provider>\n    )\n}\n\nexport const useGlobalContext = () =>{\n    return useContext(GlobalContext)\n}","/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/styles/GlobalStyle.js",[],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/utils/useWindowSize.js",[],[],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/utils/Icons.js",[],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/utils/menuItems.js",[],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/History/History.js",[],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/utils/dateFormat.js",[],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/components/Navbar/Navbar.js",["69","70"],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/screens/dashboard.js",[],[],"/home/codespace/BudgetBuddy-FinanceTracker-2/frontend/src/screens/incomes.js",[],[